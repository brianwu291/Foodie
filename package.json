{
  "name": "mysql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "set-db-config": "node createConfigFileForSequelize.js",
    "prestart-dev": "export NODE_ENV=development && yarn set-db-config",
    "prestart-test": "export NODE_ENV=test && yarn set-db-config",
    "run-server": "nodemon index.js",
    "start-dev": "yarn run-server",
    "start-test": "yarn run-server",
    "create-new-migrate": "sh ./scripts/create_new_migration_file.sh",
    "migrate-dev": "yarn prestart-dev && npx sequelize-cli db:migrate --env development",
    "migrate-test": "yarn prestart-test && npx sequelize-cli db:migrate --env test"
  },
  "author": "BrianWu",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.2",
    "sequelize": "^6.12.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.3.0",
    "sequelize-mig": "^3.1.3"
  }
}
